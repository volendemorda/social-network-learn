\";\r\nimport {compose} from \"redux\";\r\n\r\n\r\nconst mapStateToProps = (state)=>{\r\n    return {\r\n        MessagePage: state.MessagePage,\r\n        AuthPage: state.AuthPage.isAuth\r\n    }\r\n}\r\nconst mapDispatchToProps = (dispatch)=>{\r\n    return{\r\n        updateMessageDialogs: (body)=>dispatch(updateMessageCreator(body)),\r\n        onSendMessage: ()=>dispatch(addMessageCreator())\r\n    }\r\n}\r\n\r\nexport default compose(\r\n    connect(mapStateToProps,mapDispatchToProps),\r\n    withAuthRedirect\r\n)(Dialogs)\r\n","C:\\Users\\Администратор\\Documents\\#learnReact\\01-lessons\\learn-react-it\\src\\components\\Header\\headerContainer.jsx",[],"C:\\Users\\Администратор\\Documents\\#learnReact\\01-lessons\\learn-react-it\\src\\components\\Sidebar\\sidebar.jsx",[],"C:\\Users\\Администратор\\Documents\\#learnReact\\01-lessons\\learn-react-it\\src\\components\\Header\\reselect.header.js",[],"C:\\Users\\Администратор\\Documents\\#learnReact\\01-lessons\\learn-react-it\\src\\hoc\\withAuthRedirect.js",[],"C:\\Users\\Администратор\\Documents\\#learnReact\\01-lessons\\learn-react-it\\src\\components\\dialogs\\dialogs.jsx",["183"],"import React from 'react'\r\nimport './dialog.css'\r\nimport Message from \"./components/message\";\r\nimport User from \"./components/users\";\r\nimport {Redirect} from \"react-router-dom\";\r\n\r\n\r\n\r\nconst Dialogs = (props)=>{\r\n    const generateMessage = props.MessagePage.message.map(m=><Message Messeges = {m.message} key={m.id} mesId = {m.id}/>)\r\n    const generateUser = props.MessagePage.user.map(user=><User name={user.name} key={user.id} id={user.id}/>)\r\n    let newMessageBody = props.MessagePage.newMessageText\r\n    const onSendMessageClick=()=>{props.onSendMessage()}\r\n    const onchangeMessage = (event)=>{\r\n        let body = event.target.value\r\n        props.updateMessageDialogs(body)\r\n    }\r\n  return(\r\n    <div className=\"dialog__wrapper main\">\r\n        <div className=\"container\">\r\n         